@charset "utf-8";
/* CSS Document */

body {background-image:url(img/happy.jpg);
		background-size:cover;}

#logo{ background-image:url(img/herb.png);
		width:100px;
		height:125px;
		background-size:cover;
	
}

.child{
	display: inline-block;
}
		
.ital { font-family:"Alexandra Zeferino One";
		font-size:87pt;
		color:#FFF;}


.topmenu {width:100%;
			height:100px;
			background-color:rgba(255,255,255,0.63);
			}
.topmenu ul { list-style-type:none;
			padding-left: 15%;
			width:80%;}
.topmenu li { display:block;
			float:left;
			padding-left: 20px;
			padding-top:35px;
			min-height:40px;
			font-size: 20pt;
			font-family:"BenderRegular";
			} 
			
.main{font-size:20pt;
		color:#fff;
		margin-left:16%;
		width:40%;
		background-color:rgba(255,255,255,0.63);
		font-family: "MiriadPro";}

			
a:link {color: #000000;
		text-decoration:none;}
a:visited {color: #2D2D2D;
			text-decoration:none;}
a:hover {   color:#FFF;
			text-decoration:none;}
a:active {color:#fff;
			text-decoration: underline;}

@font-face {
    font-family: "BenderRegular";
    src: url(fonts/BenderRegular/BenderRegular.ttf);
    font-style: normal;
    font-weight: 200;
}
@font-face {
    font-family: "MiriadPro";
    src: url(fonts/MyriadPro-Regular.otf);
    font-style: 800;
    font-weight: 800;
}

@font-face {
    font-family: "Alexandra Zeferino One";
    src: url(fonts/Alexandra%20Zeferino%20One.ttf);
    font-style: normal;
    font-weight: normal;
}


		